<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.beneway.common.dao.schemeagency.SchemeAgencyDao">
    <resultMap id="res" type="com.beneway.common.entity.schemeagency.SchemeAgency"/>

    <select id="getSchemeInPeriod" resultMap="res">
        select sa.* from  scheme_agency as sa where now() between  sa.start_time and sa.over_time
    </select>

</mapper>