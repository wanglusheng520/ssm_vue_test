<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.beneway.common.dao.answerrecord.AnswerRecordDao">
    <select id="newest" resultType="com.beneway.common.entity.answerrecord.AnswerRecord">
        select * from answer_record where sa_id = #{id} ORDER BY create_time desc LIMIT 1
    </select>
</mapper>