<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.beneway.common.dao.startagency.StartAgencyDao">
    <resultMap id="res" type="com.beneway.common.entity.startagency.StartAgency"/>

    <select id="queryPage" resultType="com.beneway.common.entity.startagency.vo.StartAgencyVo">
        SELECT
            s.*,u.unit_name
        FROM
            start_agency AS s
            left join sys_unit u on s.unit_id = u.id
        ORDER BY
            s.type DESC
    </select>

</mapper>
