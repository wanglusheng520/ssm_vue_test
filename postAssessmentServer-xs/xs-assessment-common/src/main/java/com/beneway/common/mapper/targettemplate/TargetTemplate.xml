<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.beneway.common.dao.targettemplate.TargetTemplateDao">
    <resultMap id="res" type="com.beneway.common.entity.targettemplate.TargetTemplate"></resultMap>


    <select id="queryPage" resultMap="res">
        select
            tt.id , tt.create_time , tt.template_name , tt.target , tt.template_remark , tt.type
        from
            target_template as tt
        where 1 = 1
            <if test="param.search != null and param.search != ''">
                and tt.template_name like concat('%', #{param.search}, '%')
            </if>
    </select>

</mapper>
