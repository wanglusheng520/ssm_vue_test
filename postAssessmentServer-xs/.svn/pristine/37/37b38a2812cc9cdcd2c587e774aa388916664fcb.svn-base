<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.beneway.common.dao.prounitovertime.ProUnitOvertimeDao">

    <resultMap id="res" type="com.beneway.common.entity.prounitovertime.ProUnitOvertime"></resultMap>

    <select id="getOne" resultMap="res">
        select over_time  , `year` , period from pro_unit_overtime where mark = '0' and scheme_id = #{schemeId} ORDER BY period asc limit 1
    </select>
</mapper>