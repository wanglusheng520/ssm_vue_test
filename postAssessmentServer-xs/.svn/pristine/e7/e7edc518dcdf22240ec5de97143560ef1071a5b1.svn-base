<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.beneway.common.system.dao.sys_user.SysUserDao">
    <resultMap id="res" type="com.beneway.common.system.entity.sys_user.SysUser" />

    <select id="queryPage" resultType="com.beneway.common.system.entity.sys_user.vo.SysUserVo">
        select
            su.*
        from sys_user as su
        <where>
            <include refid="del"></include>
            and su.unit_id in
            <foreach collection="params.unitIdSet" item="item" open="(" separator="," close=")">
                #{item}
            </foreach>
            <if test="params.username != null and params.username != ''">
                and su.username like concat('%', #{params.username}, '%')
            </if>
        </where>
        order by su.create_time
    </select>

    <sql id="del">
       and su.deleted = 0
    </sql>

</mapper>
